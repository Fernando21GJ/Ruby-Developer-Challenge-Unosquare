<p id="notice"><%= notice %></p>

<h1>Real Estates</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Type</th>
      <th>Street</th>
      <th>External number</th>
      <th>Internal number</th>
      <th>Neighborhood</th>
      <th>String</th>
      <th>City</th>
      <th>Country</th>
      <th>Rooms</th>
      <th>Bathrooms</th>
      <th>Comments</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @real_estates.each do |real_estate| %>
      <tr>
        <td><%= real_estate.name %></td>
        <td><%= real_estate.type %></td>
        <td><%= real_estate.street %></td>
        <td><%= real_estate.external_number %></td>
        <td><%= real_estate.internal_number %></td>
        <td><%= real_estate.neighborhood %></td>
        <td><%= real_estate.string %></td>
        <td><%= real_estate.city %></td>
        <td><%= real_estate.country %></td>
        <td><%= real_estate.rooms %></td>
        <td><%= real_estate.bathrooms %></td>
        <td><%= real_estate.comments %></td>
        <td><%= link_to 'Show', real_estate %></td>
        <td><%= link_to 'Edit', edit_real_estate_path(real_estate) %></td>
        <td><%= link_to 'Destroy', real_estate, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Real Estate', new_real_estate_path %>
